<% include partials/header %>


     
            <form method="POST" action="/table/update/<%= doc._id %>">
				
				
                <label for="comname"> Company Name:</label>
                <input type="text" name="cname" value="<%= doc.companyName %>">
                 <br><br>
                <label for="Address">Address:</label>
                <input type="text" name="address"  value="<%= doc.address %>">
                <br><br><br>
                <label for="clastname">Last Name:</label>
                <input type="text" name="clastname"  value="<%= doc.clastname %>">
                <br><br><br>
                <label for="website">Web site:</label>
                <input type="text" name="website" value="<%= doc.website %>" >
                <br><br><br>
                Location:
                <input type="text" name="location" value="<%= doc.location %>" >
                <br><br><br>
                <label for="cemail">Company email:</label>
                <input type="email" name="cemail" value="<%= doc.cemail %>" >
                <br><br><br>
                <label for="cpswd">Password:</label>
                <input type="password" id="cpswd" name="cpswd" value="<%= doc.cpswd %>">
                <br><br><br>
                <label for="cdescription"> Company Description:</label>
                <textarea name="cdescription"   > <%= doc.cdescription %></textarea>
                <br><br><br>
				
				<br><button type="submit" class="btn">Update</button>
                <br><button type="reset" class="btn">Reset</button>
                </form>
				
         
       
                <% include partials/footer %>